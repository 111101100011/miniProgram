<!--pages/subject/search.wxml-->
<view class='sousuo'>
  <input type='text' 
  		 placeholder='请输入您要搜索的内容'
         bindinput='input' 
         bindconfirm='confirm'/>
  <icon type='search' class='icons'></icon>
  <view wx:for="{{list}}"  wx:key='' class='lists'>
    <text wx:if="{{item.show}}">{{item.name}}</text>
  </view>
</view>
