@import "./styles/iconfont.wxss";
/* 小程序不识别通配符
   把标签一个个设置
*/
page,
view,
text,
swiper,
swiper-item,
image,
navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
/* 主题颜色 通过变量来实现 */
page {
  /* 定义主题颜色 */
  --themeColor: #eb4450;
  /* 统一字体大小 假设设计稿 大小375px */
  font-size: 28rpx;
}
image {
  width: 100%;
}
